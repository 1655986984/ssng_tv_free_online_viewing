{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "functions": {
    "api/api.js": {
      "runtime": "edge"
    },
    "api/ikun.js": {
      "runtime": "edge"
    },
    "api/subo.js": {
      "runtime": "edge"
    },
    "api/huawei.js": {
      "runtime": "edge"
    },
    "api/jisu.js": {
      "runtime": "edge"
    },
    "api/360.js": {
      "runtime": "edge"
    },
    "api/wolong.js": {
      "runtime": "edge"
    }
  },
  "routes": [
    { "handle": "filesystem" },
    
    { "src": "/api/api.php/(.*)", "dest": "/api/api" },
    { "src": "/ikun/(.*)", "dest": "/api/ikun" },
    { "src": "/subo/(.*)", "dest": "/api/subo" },
    { "src": "/huawei/(.*)", "dest": "/api/huawei" },
    { "src": "/jisu/(.*)", "dest": "/api/jisu" },
    { "src": "/360/(.*)", "dest": "/api/360" },
    { "src": "/wolong/(.*)", "dest": "/api/wolong" },

    {
      "src": "/(.*)",
      "has": [
        {
          "type": "header",
          "key": "accept",
          "value": "text/html"
        }
      ],
      "dest": "/index.html"
    }
  ]
}
