{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    { "handle": "filesystem" },

    { "src": "/api/heimuer/(.*)", "dest": "/api/heimuer" },
    { "src": "/ikun/(.*)", "dest": "/api/ikun" },
    { "src": "/subo/(.*)", "dest": "/api/subo" },
    { "src": "/huawei/(.*)", "dest": "/api/huawei" },
    { "src": "/jisu/(.*)", "dest": "/api/jisu" },
    { "src": "/360/(.*)", "dest": "/api/360" },
    { "src": "/wolong/(.*)", "dest": "/api/wolong" },

    { "src": "/api/proxy/([^/]+)/(.*)", "dest": "/api/proxy?service=$1&0=$2" },

    {
      "src": "/(.*)",
      "has": [
        {
          "type": "header",
          "key": "accept",
          "value": "text/html"
        }
      ],
      "dest": "/index.html"
    }
  ]
}
