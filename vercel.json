{
  "version": 2,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT" },
        { "key": "Access-Control-Allow-Headers", "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version" },
        { "key": "Set-Cookie", "value": "" },
        { "key": "Cookie", "value": "" }
      ]
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    
    {
      "src": "/api/api.php/provide/vod/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept",
        "Set-Cookie": "",
        "Cookie": ""
      },
      "dest": "https://www.heimuer.tv/api.php/provide/vod/$1"
    },

    { 
       "src": "/api/(.*)", 
       "dest": "https://www.heimuer.tv/$1",
       "headers": {
         "Access-Control-Allow-Origin": "*",
         "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
         "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept",
         "Set-Cookie": "",
         "Cookie": ""
       }
     },
    { 
       "src": "/ikun/(.*)",   
       "dest": "https://ikunzy.net/$1",
       "headers": {
         "Access-Control-Allow-Origin": "*",
         "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
         "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept",
         "Set-Cookie": "",
         "Cookie": ""
       }
     },
    { 
       "src": "/subo/(.*)",   
       "dest": "https://www.suboziyuan.net/$1",
       "headers": {
         "Access-Control-Allow-Origin": "*",
         "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
         "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept",
         "Set-Cookie": "",
         "Cookie": ""
       }
     },
    { 
       "src": "/huawei/(.*)", 
       "dest": "https://cjhwba.com/$1",
       "headers": {
         "Access-Control-Allow-Origin": "*",
         "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
         "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept",
         "Set-Cookie": "",
         "Cookie": ""
       }
     },
     { 
       "src": "/jisu/(.*)",   
       "dest": "https://www.jisuzy.com/$1",
       "headers": {
         "Access-Control-Allow-Origin": "*",
         "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
         "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept",
         "Set-Cookie": "",
         "Cookie": ""
       }
     },
     { 
       "src": "/360/(.*)",    
       "dest": "https://360zy5.com/$1",
       "headers": {
         "Access-Control-Allow-Origin": "*",
         "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
         "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept",
         "Set-Cookie": "",
         "Cookie": ""
       }
     },
     { 
       "src": "/wolong/(.*)", 
       "dest": "https://wolongzyw.com/$1",
       "headers": {
         "Access-Control-Allow-Origin": "*",
         "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
         "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept",
         "Set-Cookie": "",
         "Cookie": ""
       }
     },

    {
      "src": "/(.*)",
      "has": [
        {
          "type": "header",
          "key": "accept",
          "value": "text/html"
        }
      ],
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "headers": {
        "X-Frame-Options": "DENY",
        "X-XSS-Protection": "1; mode=block",
        "X-Content-Type-Options": "nosniff",
        "Strict-Transport-Security": "max-age=31536000; includeSubDomains"
      },
      "continue": true
    }
  ]
}
